<div *ngIf="alert">
    <p>{{alert.fromCurrency}}</p>

    <label>From Currency:</label> <input [(ngModel)]="alert.from_currency" />
    <label>To Currency:</label> <input [(ngModel)]="alert.to_currency" />
    <label>Lower Rate:</label> <input [(ngModel)]="alert.lower_rate" />
    <label>Upper Rate:</label> <input [(ngModel)]="alert.upper_rate" />
    <button (click)="save();">
    Save
  </button>

</div>
<button (click)="goBack()">Back</button>